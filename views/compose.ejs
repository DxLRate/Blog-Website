<%- include("partials/header.ejs"); -%>

<h1>Compose</h1>
<form action="/compose" method="post">
    <div class="form-group">
        <label for="titleName" class="form-label">Title</label>
        <input type="text" id="titleName" class="form-control" name="postTitle">
    </div>
    <div class="form-group">
        <label for="postName" class="form-label">Post</label>
        <textarea name="postBody" id="postName" class="form-control" rows="5"></textarea>
    </div>
    
    <button type="submit" name="button" class="btn btn-primary">Publish</button>
    
    
</form>


<%- include("partials/footer"); -%>